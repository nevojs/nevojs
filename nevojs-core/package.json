    "jest": "25.1.0",
    "rollup": "1.31.0",
    "rollup-plugin-terser": "5.2.0",
    "rollup-plugin-typescript2": "0.25.3",
    "rollup-plugin-uglify": "6.0.4",
    "ts-jest": "25.1.0",
    "typedoc": "next",
